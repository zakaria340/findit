<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    {#<script async src="https://cdn.ampproject.org/v0.js"></script>#}
    <title>{% block title %}Cherchi.ma Moteur de recherche des annonces prix au Maroc{% endblock %}</title>

    {% block metadescription %}

    {% endblock %}

    {% block canonical_rel %}
        <link rel="canonical" href="{{ url('homepage') }}"/>{% endblock %}
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    {#
        <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
    #}

    {% block stylesheets %}
        <link href='https://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/index2.css') }}" media="all">
        <link rel="stylesheet" media="screen, projection" href="{{ asset('css/master.css') }}"/>
        <link rel="stylesheet" media="screen, projection" href="{{ asset('css/infinite.css') }}"/>
        <link rel="stylesheet" media="screen, projection" href="{{ asset('css/adapt.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/custom2.css') }}" type="text/css"/>
    {% endblock %}

    {% block javascript %}
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
      <script src="{{ asset('js/masonry.js') }}"></script>
      <script src="{{ asset('js/main.js') }}"></script>
    {% endblock %}

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-96039903-1', 'auto');
        ga('send', 'pageview');

    </script>
</head>
<body>
<div class="group" id="header">
    <div class="group" id="header-inner">
        <div id="logo"><a title="Cherchi.ma Moteur de recherche des annonces prix au Maroc" href="{{ url('homepage') }}">Cherchima</a></div>
    </div>
</div>

{% block topHeader %}

{% endblock %}

<div class="search">
    {{ render(controller("AppBundle:Default:search", app.request.attributes.get('_route_params'))) }}
</div>

<div id="wrap">
    <div id="content" class="group">

        <div id="main" class="content container main-full">
            {% block body %}

            {% endblock %}
        </div>
    </div>
</div>


<div id="footer">
    <div id="footer-inner">
        <div class="group">
            <div class="footer-mod">
                <p class="logo"><br>Cherchi.ma</p>
                <p class="about">Le moteur de recherche des annonces prix au maroc. Comparer les annonces dans plusieurs sites Voitures, Appartement, Immobilier ...</p>
            </div>
            <div class="footer-mod">
                {{ render(controller("AppBundle:Default:footer")) }}
            </div>
        </div>
        <p>Copyright Cherchi.ma Moteur de recherche des annonces au maroc.</p>
    </div>
</div>
</body>
</html>